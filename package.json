{
  "name": "commissioner",
  "version": "2.3.0",
  "description": "Configuration helper for node.js services running with consul and/or Docker",
  "license": "MIT",
  "homepage": "https://github.com/mthenw/commissioner",
  "keywords": [
    "docker",
    "consul",
    "registrator",
    "service-discovery"
  ],
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha test/*.js --require should --reporter spec",
    "lint": "./node_modules/.bin/eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mthenw/commissioner.git"
  },
  "bugs": {
    "url": "https://github.com/mthenw/commissioner/issues"
  },
  "devDependencies": {
    "eslint": "^0.21.0",
    "mocha": "^2.1.0",
    "should": "^5.0.0",
    "sinon": "^1.13.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "strict": [
        2,
        "never"
      ],
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "no-shadow": 0
    },
    "globals": {
      "describe": false,
      "it": false,
      "beforeEach": false,
      "afterEach": false
    }
  }
}
